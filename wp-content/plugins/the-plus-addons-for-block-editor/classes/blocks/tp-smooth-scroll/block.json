        "accMax": {
            "type": "string",
            "default": "3"
        },
        "keySupp": {
            "type": "boolean",
            "default": true
        },
        "arrowSco": {
            "type": "string",
            "default": "50"
        },
        "touchSupp": {
            "type": "boolean",
            "default": true
